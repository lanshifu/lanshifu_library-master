<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <library.lanshifu.com.lsf_library.commwidget.FlowLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/spacing_2x"
        android:paddingRight="@dimen/spacing_2x">

        <Button
            android:id="@+id/btn_single"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="单选窗" />

        <Button
            android:id="@+id/btn_multi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="多选框" />

        <Button
            android:id="@+id/btn_base"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="辅助功能查看类名" />

        <Button
            android:id="@+id/btn_mult"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="wifi密码查看"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/toolbar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="tabActivity"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/popmenu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="菜单" />

        <Button
            android:id="@+id/pagerfragment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="pagerfragment"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/bt_contentprovider"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="内容提供者" />

        <Button
            android:id="@+id/bt_voice"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="录音" />

        <Button
            android:id="@+id/bt_photopicker"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="图片选择" />

        <Button
            android:id="@+id/bt_slid_pager"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="卡片pager"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/btn_viewpager"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="viewpager切换"
            android:textAllCaps="false" />
 <Button
            android:id="@+id/btn_databinding"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="mvvm"
            android:textAllCaps="false" />

    </library.lanshifu.com.lsf_library.commwidget.FlowLayout>


</LinearLayout>
